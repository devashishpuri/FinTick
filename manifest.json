
{
    "manifest_version": 2,
    "name": "FinTick",
    "description": "Finatial Ticker for Stocks and Cryptos",
    "version": "1.0",
    "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
    },
    "permissions": [],
    "content_security_policy": "script-src ‘self’ ‘unsafe-eval’; object-src ‘self’",
    "web_accessible_resources": [
    "assets/css/*",
    "assets/js/*",
    "assets/fonts/*"
    ]
   }